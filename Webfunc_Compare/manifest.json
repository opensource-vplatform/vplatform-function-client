{
    "groupId": "com.toone.v3.platform",
    "code": "Webfunc_Compare",
    "version":"3.4.0",
    "plugins": [{
        "type": "function",
        "code": "Compare",
        "name": "比较函数",
        "desc": "比较两个字符串是否一致,返回比较结果，相等则返回True。\n参数1--比较串(字符串类型)；\n参数2--被比较串(字符串类型)；\n参数3--是否忽略大小写，true为忽略大小写(布尔类型)；\n返回值为布尔类型。",
        "example":"Compare(\"ab\",\"Ab\",true)，返回值为True",
        "entry": "com.toone.v3.platform.function.client.Compare.main",
        "defineUrl": "./dist/index.js",
        "debugUrl":"./test/index.js",
        "inputs": [
        {
            "type": "text",
            "desc": "比较的字符串",
            "required": true
        },
        {
            "type": "text",
            "desc": "被比较的字符串",
            "required": true
        },
        {
            "type": "boolean",
            "desc": "是否忽略大小写",
            "required": true
        }
        ],
        "output": {
            "type": "boolean",
            "desc": ""
        }
    }]
}